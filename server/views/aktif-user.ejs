<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="anto_stiek@yahoo.com">
    <meta name="author" content="Anto">
    <meta name="keyword" content="Anto">
    <link rel="shortcut icon" href="img/favicon.png">

     <title>Sistem Informasi Pelayanan Akademik</title>

    <!-- Bootstrap CSS -->    
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <!-- bootstrap theme -->
    <link href="/css/bootstrap-theme.css" rel="stylesheet">
    <!--external css-->
    <!-- font icon -->
    <link href="/css/elegant-icons-style.css" rel="stylesheet" />
    <link href="/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Custom styles -->
    <link href="/css/style.css" rel="stylesheet">
    <link href="/css/style-responsive.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
      <script src="js/lte-ie7.js"></script>
    <![endif]-->
  </head>

  <body>
  <!-- container section start -->
  <section id="container" class="">
      <!--header start-->
      <header class="header dark-bg">
            <div class="toggle-nav">
                <div class="icon-reorder tooltips" data-original-title="Toggle Navigation" data-placement="bottom"><i class="icon_menu"></i></div>
            </div>

            <!--logo start-->
            <a href="/home" class="logo">Sistem Informasi Pelayanan Akademik</a>
            <!--logo end-->

            <div class="nav search-row" id="top_menu">               
            </div>

            <div class="top-nav notification-row">                
                <!-- notificatoin dropdown start-->
                <ul class="nav pull-right top-menu">
                    <!-- user login dropdown start-->
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <span class="profile-ava">
                                <img alt="" src="img/avatar-mini4.jpg">
                            </span>
                            <span class="username">Falah</span>
                            <b class="caret"></b>
                        </a>
                    </li>
                    <!-- user login dropdown end -->
                </ul>
                <!-- notificatoin dropdown end-->
            </div>
      </header>      
      <!--header end-->

      <!--sidebar start-->
      <aside>
          <div id="sidebar"  class="nav-collapse ">
              <!-- sidebar menu start-->
              <ul class="sidebar-menu">                
                  <li class="active">
                      <a class="" href="/home">
                          <span>Dashboard</span>
                      </a>
                  </li>
                  <li class="">
                      <a class="" href="/surat/aktif">
                          <span>Surat Keterangan Aktif</span>
                      </a>
                  </li>
                  <li class="">
                      <a class="" href="/surat/akhir">
                          <span>Surat Permintaan Data Untuk Tugas Akhir</span>
                      </a>
                  </li> 
                  <li class="">
                      <a class="" href="/surat/matkul">
                          <span>Surat Permintaan Data Untuk Tugas Matakuliah</span>
                      </a>
                  </li>                  
              </ul>
              <!-- sidebar menu end-->
          </div>
      </aside>
      <!--sidebar end-->

      <!--main content start-->
      <section id="main-content">
          <section class="wrapper">
		  <div class="row">
				<div class="col-lg-12">
					<h3 class="page-header"><i class="fa fa-table"></i> Status Pengajuan Surat</h3>
					<ol class="breadcrumb">
						<li><i class="fa fa-home"></i><a href="/home">Home</a></li>
						<li><i class="fa fa-table"></i>Surat Keterangan Aktif</li>
						<li><i class="fa fa-th-list"></i>Data</li>
					</ol>
				</div>
			</div>
              <!-- page start-->
              <div class="row">
                  <div class="col-lg-12">
                      <section class="panel">
                      <a class="btn btn-primary" href="/surat/aktif/form" title=""><span class="icon_plus_alt2"></span> Tambah</a>
                          <table class="table table-striped table-advance table-hover">
                           <tbody>
                              <tr>
                                 <th>No. Ajuan</th>
                                 <th>Tanggal Ajuan</th>
                                 <th>Nama Mahasiswa</th>
                                 <th>NIM</th>
                                 <th>Status</th>
                                 <th>Tgl. Pengambilan</th>
                                 <th>Keterangan</th>
                                 <!-- <th>Detail</th> -->
                              </tr>
                              <% for(var i=0; i < data.length; i++) { %>
                              <tr>
                                 <td><%= data[i].nomor %></td>
                                 <td><%= data[i].tanggalDiajukan.toUTCString() %></td>                                 
                                 <td><%= data[i].idMahasiswa.nama %></td>
                                 <td><%= data[i].idMahasiswa.nim %></td>
                                 <td><%= data[i].status %></td>
                                 <td><%= data[i].tanggalSelesai.toUTCString() %></td>
                                 <td><%= data[i].tujuanPembuatan %></td>
<!--                                  <td>
                                  <div class="btn-group">
                                      <a class="btn btn-primary" href="detail-aktif.html"><i class="fa fa-th-list"></i></a>
                                  </div>
                                  </td> -->
                              </tr>   
                              <% } %>                          
                           </tbody>
                        </table>
                      </section>
                  </div>
              </div>
              <!-- page end-->
          </section>
      </section>
      <!--main content end-->
  </section>
  <!-- container section end -->
    <!-- javascripts -->
    <script src="/js/jquery.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <!-- nicescroll -->
    <script src="/js/jquery.scrollTo.min.js"></script>
    <script src="/js/jquery.nicescroll.js" type="text/javascript"></script>
    <!--custome script for all page-->
    <script src="/js/scripts.js"></script>


  </body>
</html>
